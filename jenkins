pipeline {
    agent any

   

    stages {
        stage('Build') {
            steps {
                // Get some code from a GitHub repository
               git 'https://github.com/Kishoor187/apollohospital.git'

                

                
            }

            
        }
        
        stage{
            
            
            steps{
                // Run Maven on a Unix agent.
                bat "mvn clean verify"
                
                
            }
        }
    }
}
